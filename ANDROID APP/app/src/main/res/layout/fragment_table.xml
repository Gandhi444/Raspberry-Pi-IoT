<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.table.TableFragment">


    <com.jjoe64.graphview.GraphView
        android:id="@+id/Joy"
        android:layout_width="215dp"
        android:layout_height="215dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.842"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="725dp"
        android:layout_height="78dp"
        android:layout_marginStart="1dp"
        android:layout_marginBottom="1dp"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="parent">

        <CheckBox
            android:id="@+id/checkBoxTemperature"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:text="Temperatura"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="334dp" />

        <CheckBox
            android:id="@+id/checkBoxHumidity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:text="Wilgotność"
            app:layout_constraintEnd_toStartOf="@+id/checkBox5"
            tools:layout_editor_absoluteY="334dp" />

        <CheckBox
            android:id="@+id/checkBoxPressure"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:text="Ciśnienie"
            app:layout_constraintEnd_toStartOf="@+id/checkBox6"
            tools:layout_editor_absoluteY="334dp" />

        <CheckBox
            android:id="@+id/checkBoxYaw"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:text="Yaw"
            app:layout_constraintEnd_toStartOf="@+id/checkBox3"
            tools:layout_editor_absoluteY="334dp" />

        <CheckBox
            android:id="@+id/checkBoxPitch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:text="Pitch"
            app:layout_constraintEnd_toStartOf="@+id/checkBox4"
            tools:layout_editor_absoluteY="334dp" />

        <CheckBox
            android:id="@+id/checkBoxRoll"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:text="Roll"
            app:layout_constraintEnd_toEndOf="parent"
            tools:layout_editor_absoluteY="334dp" />
    </LinearLayout>

    <TableLayout
        android:id="@+id/MainTable"
        android:layout_width="325dp"
        android:layout_height="192dp"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.079"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.801" />

</androidx.constraintlayout.widget.ConstraintLayout>